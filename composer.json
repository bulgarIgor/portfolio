{
  "name": "drupal-composer/drupal-project",
  "description": "Project template for Drupal 8 projects with composer",
  "type": "project",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "",
      "role": ""
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6",
    "drupal-composer/drupal-scaffold": "^2.2",
    "drupal/admin_toolbar": "~1.0",
    "drupal/better_exposed_filters": "^3.0@alpha",
    "drupal/bg_image_formatter": "^1.0",
    "drupal/config_direct_save": "^1.0",
    "drupal/config_installer": "^1.0",
    "drupal/console": "~1.0",
    "drupal/contact_block": "^1.0",
    "drupal/core": "~8.0",
    "drupal/ctools": "^3.0@alpha",
    "drupal/devel": "1.x-dev",
    "drupal/ds": "^3.1",
    "drupal/exclude_node_title": "^1.0@beta",
    "drupal/field_group": "^1.0@RC",
    "drupal/image_effects": "^1.0",
    "drupal/libraries": "3.x-dev",
    "drupal/metatag": "~1.0",
    "drupal/owlcarousel": "^1.0@alpha",
    "drupal/pathauto": "^1.0@RC",
    "drupal/redirect": "^1.0@alpha",
    "drupal/responsive_menu": "^2.4",
    "drupal/sendgrid_integration": "^1.0",
    "drupal/smart_trim": "^1.0",
    "drupal/social_media_links": "^2.5",
    "drupal/twig_xdebug": "^1.0",
    "drush/drush": "~8.0",
    "webflo/drupal-finder": "^0.3.0",
    "webmozart/path-util": "^2.3"
  },
  "require-dev": {
    "behat/mink": "~1.7",
    "behat/mink-goutte-driver": "~1.2",
    "jcalderonzumba/gastonjs": "~1.0.2",
    "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
    "mikey179/vfsstream": "~1.2",
    "phpunit/phpunit": ">=4.8.28 <5",
    "symfony/css-selector": "~2.8"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php",
      "scripts/composer/PrepareForPantheon.php"
    ]
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "prepare-for-pantheon": "DrupalProject\\composer\\PrepareForPantheon::prepareForPantheon",
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ]
  },
  "extra": {
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    }
  }
}
